ELINK
	Name  = "{ 54, 13, 54 + 24, 13 + 8, NavFrameHandlewithMouse, 1 },"
	Parent  = "HOTCLICK_LIST"
	InvokeOrder = AfterParent
	Help  = "Dont change the HOTCLICK_LIST order, for 80*25 mode"
    Token = "HOTCLICK_FULLSCREEN_SUPPORT" "=" "1"
